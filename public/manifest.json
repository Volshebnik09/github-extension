{
  "name": "github expansion",
  "description": "Extends the capabilities of github",
  "version": "1.1",
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],

  "permissions": ["activeTab", "scripting","storage","tabs","history"],
  "icons": {
    "32": "/images/GitHub-Mark-Light-32px.png",
    "48": "/images/GitHub-Mark-Light-64px.png",
    "128": "/images/GitHub-Mark-Light-120px-plus.png"
  },
  "action": {
    "default_icon": {
      "32": "/images/GitHub-Mark-Light-32px.png",
      "48": "/images/GitHub-Mark-Light-64px.png",
      "128": "/images/GitHub-Mark-Light-120px-plus.png"
    },
    "default_popup": "index.html"
  },
  "content_scripts":
  [
    {
      "matches":["https://github.com/*"],
      "js":["scripts/vendors.bundle.js","scripts/downloadFolder.bundle.js","scripts/index.bundle.js"],
      "run_at": "document_end"
    }
  ]

}